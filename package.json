{
  "private": true,
  "name": "remix-app-template-js",
  "description": "",
  "license": "",
  "prisma": {
    "seed": "node prisma/seed"
  },
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "concurrently \"npm run watch:css\" \"remix dev\"",
    "watch:css": "tailwindcss -i app/styles/input.css -o app/styles/tailwind.css --watch",
    "postinstall": "remix setup",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.4.1",
    "@mui/material": "^5.4.1",
    "@mui/utils": "^5.4.1",
    "@prisma/client": "^3.8.1",
    "@remix-run/react": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "bcrypt": "^5.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recharts": "^2.1.8",
    "remix": "^1.0.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.6",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "postcss": "^8.4.8",
    "prisma": "^3.8.1",
    "tailwindcss": "^3.0.23"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
